<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="appGenerateCV.CVDetailPage"
             Title="Detalle del CV">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout Padding="20">
                <Label Text="Detalle del CV" FontSize="24" HorizontalOptions="CenterAndExpand"/>

                <Label x:Name="nameLabel" FontAttributes="Bold"/>
                <Label x:Name="emailLabel"/>
                <Label x:Name="phoneLabel"/>
                <Label x:Name="addressLabel"/>
                <Label x:Name="languagesLabel"/>
                <Label x:Name="experienceLabel"/>
                <Label x:Name="educationLabel"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>
